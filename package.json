{
    "name": "property-finder-scraper",
    "version": "1.0.0",
    "description": "Property Finder Office Space Scraper",
    "main": "office_agent.cjs",
    "scripts": {
        "start": "node office_agent.cjs",
        "build": "npm install --production",
        "test": "TEST_PAGINATION=true node office_agent.cjs",
        "test:pages": "TEST_PAGINATION=true MAX_PAGES=2 node office_agent.cjs",
        "initial": "MODE=initial node office_agent.cjs",
        "daily": "MODE=update node office_agent.cjs"
    },
    "dependencies": {
        "puppeteer": "^22.8.2",
        "csv-writer": "^1.6.0",
        "@google-cloud/storage": "^7.9.0"
    },
    "engines": {
        "node": ">=18.19.1"
    },
    "private": true
}
